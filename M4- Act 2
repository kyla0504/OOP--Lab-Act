public class FindMaximumElement {
    public static void main(String[] args) {
      
        int[] numbers = {45, 20, 88, 92, 54, 31};

        int max = numbers[0];

        for (int i = 1; i < numbers.length; i++) {
            if (numbers[i] > max) {
                max = numbers[i];
            }
        }

        System.out.println("The maximum element in the array is: " + max);
    }
}
