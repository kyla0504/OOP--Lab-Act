import java.util.ArrayList;

public class NameList {
    public static void main(String[] args) {

        ArrayList<String> names = new ArrayList<>();

        names.add("Hanna");
        names.add("Bron");
        names.add("Jessa");
        names.add("Carlo");

        System.out.println("List of Names:");
        for (String name : names) {
            System.out.println("- " + name);
        }
    }
}
